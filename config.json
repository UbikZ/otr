{
  "api": {
    "version": 1
  },
  "env": {
    "debug": false,
    "development": {
      "port": 3000,
      "mongo": {
        "uri": "mongodb://localhost:27017/otr-dev"
      }
    },
    "staging": {
      "port": 3001,
      "mongo": {
        "uri": "mongodb://localhost:27017/otr-stage"
      }
    },
    "production": {
      "port": 3000,
      "mongo": {
        "uri": "mongodb://localhost:27017/otr-prod"
      }
    }
  },
  "bin": {
    "phantomjs": "phantomjs"
  },
  "path": {
    "node": "./node_modules",
    "public": "./public",
    "export": "./public/exports",
    "scripts": "./scripts",
    "coverage": "./coverage",
    "logs": "./logs",
    "client": {
      "views": "./public/views",
      "app": "./src/client/app",
      "print": "./src/client/print",
      "test": "./src/client/test",
      "coverage": "./coverage/front"
    },
    "server": {
      "root": "./src/server",
      "controller": "./src/server/controllers",
      "socket": "./src/server/sockets",
      "model": "./src/server/models",
      "prototype": "./src/server/prototypes",
      "test": "./src/server/test",
      "coverage": "./coverage/back"
    }
  },
  "particularities": {
    "jquery-slimscroll": "jquery-slimscroll/jquery.slimscroll"
  }
}